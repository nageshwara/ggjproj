package {		import flash.display.MovieClip;			public class mapEditor extends MovieClip {						public function mapEditor() {						getPositions();		}				public function getPositions():void{						var theMapBlocks = mapHolderMc.numChildren;			trace("Hello World" + theMapBlocks);						for (var i:Number = 0; i< theMapBlocks; i++){								var theBlock = mapHolderMc.getChildAt(i);				trace("<block xPos = '" + theBlock.x + "' " + "yPos = '" + theBlock.y + "' />");				// trace(theBlock.y);			}					}					}	}